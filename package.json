{
  "name": "@juntossomosmais/time-out-market",
  "description": "A market of shared JavaScript libraries to help you save time and effort",
  "license": "MIT",
  "private": true,
  "npmClient": "npm",
  "engines": {
    "yarn": "please-use-npm",
    "npm": ">=8.x",
    "node": ">=16.x"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "pre-commit": "lint-staged",
    "start": "npx nx run-many --target=start --maxParallel=10",
    "test": "npx nx run-many --target=test --parallel",
    "test:ci": "npx nx affected --target=test:ci --base=origin/main --parallel --verbose",
    "test:ci-merge": "npx nx affected --target=test:ci --base=origin/main~1 --head=origin/main --parallel --verbose"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.25.1",
    "@juntossomosmais/atomium-tokens": "^2.0.0",
    "@nx/jest": "20.8.1",
    "@nx/js": "20.8.1",
    "@nx/vite": "20.8.1",
    "@nx/web": "20.8.1",
    "@nx/workspace": "20.8.1",
    "@swc-node/register": "~1.10.10",
    "@swc/core": "~1.11.24",
    "@swc/helpers": "~0.5.15",
    "@types/jest": "^29.5.14",
    "@types/node": "22.15.3",
    "@types/object-hash": "^3.0.6",
    "@types/pako": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "babel-loader": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.1",
    "nx": "20.8.1",
    "prettier": "3.5.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-order": "^7.0.0",
    "ts-jest": "^29.3.2",
    "ts-node": "10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.4",
    "vite-plugin-dts": "^4.5.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "object-hash": "^3.0.0",
    "pako": "^2.1.0",
    "tslib": "^2.8.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.40.0"
  }
}
