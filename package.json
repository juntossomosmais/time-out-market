{
  "name": "@juntossomosmais/time-out-market",
  "description": "A market of shared JavaScript libraries to help you save time and effort",
  "license": "MIT",
  "private": true,
  "npmClient": "npm",
  "engines": {
    "yarn": "please-use-npm",
    "npm": ">=8.x",
    "node": ">=16.x"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "pre-commit": "lint-staged",
    "start": "npx nx run-many --target=start --maxParallel=10",
    "test": "npx nx run-many --target=test --parallel",
    "test:ci": "npx nx affected --target=test:ci --base=origin/main --parallel",
    "test:ci-merge": "npx nx affected --target=test:ci --base=origin/main~1 --head=origin/main --parallel"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.19.0",
    "@juntossomosmais/atomium-tokens": "^2.0.0",
    "@nx/jest": "20.7.0",
    "@nx/js": "20.7.0",
    "@nx/vite": "20.7.0",
    "@nx/web": "20.7.0",
    "@nx/workspace": "20.7.0",
    "@swc-node/register": "~1.10.10",
    "@swc/core": "~1.11.13",
    "@swc/helpers": "~0.5.15",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.15",
    "@types/object-hash": "^3.0.6",
    "@types/pako": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "babel-loader": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.0",
    "nx": "20.7.0",
    "prettier": "3.5.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-order": "^6.0.4",
    "ts-jest": "^29.3.1",
    "ts-node": "10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.2.4",
    "vite-plugin-dts": "^4.5.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.scss": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "object-hash": "^3.0.0",
    "pako": "^2.1.0",
    "tslib": "^2.8.1"
  }
}
