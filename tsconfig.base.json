{
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": ".",
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": true,
    "lib": ["es6"],
    "paths": {
      "@juntossomosmais/http-front-cache": [
        "packages/http-front-cache/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "**/*.spec.ts", "**/__tests__/**"]
}
